<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="mingtak.extramenu">

    <configlet
        title="Extra main menu setting"
        action_id="mingtak.extramenu.mainmenu"
        appId="mingtak.extramenu"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@mainmenusetting"
        icon_expr="string:/favicon.ico"
        visible="True"
        i18n:attributes="title">
            <permission>Manage portal</permission>
    </configlet>
</object>
